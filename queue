class Node:
  
  def __init__(self, data):
    self.data = data
    self.prev = None
    
  def get_data(self):
    return self.data
  
  def get_prev(self):
    return self.prev
  
  def set_prev(self, prev):
    self.prev = prev
    
class Queue:
  def __init__()
  def enqueque()
  def print_queue()
  def dequeue()
  
queue = Queue()
queue.enqueue("Apple")
queue.enqueue("Kiwi")
queue.enqueue("Banana")
queue.print_queue()

node = queue.dequeue()
print("Dequeue: " + node.get_data())
node = queue.dequeue()
print("Dequeue: " + node.get_data())

queue.enqueue("Melon")
queue.print_queue()
